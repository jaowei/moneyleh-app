{
  "name": "moneyleh-app",
  "type": "module",
  "scripts": {
    "dev": "bun run --hot src/index.ts",
    "start": "bun run src/index.ts",
    "migrate:better-auth": "bunx --bun @better-auth/cli migrate",
    "generate:migrations": "bun drizzle-kit generate",
    "migrate:db": "bun run ./src/db/migrate.ts",
    "seed:db": "bun run ./src/db/seed.ts"
  },
  "dependencies": {
    "better-auth": "^1.3.4",
    "drizzle-orm": "^0.44.4",
    "drizzle-zod": "^0.8.3",
    "hono": "^4.8.10",
    "mupdf": "^1.26.4",
    "resend": "^4.7.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.31.4"
  }
}